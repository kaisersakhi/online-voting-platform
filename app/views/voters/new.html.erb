<div class="main-body">
  <%= form_with(url: voters_path, method: :post, class: 'login-register-form') do %>

    <div class="mb-3">
      <label for="emailId" class="form-label">New Voter ID</label>
      <input
        name="voter_id"
        type="text"
        class="form-control"
        id="emailId"
        placeholder="example : UD9FJDU19"
      >
    </div>
    <div class="mb-3">
      <label for="password" class="form-label">Voter Password</label>
      <input name="voter_password" type="password" class="form-control" id="password">
    </div>
    <button type="submit" class="btn btn-primary">Add New Voter</button>
  <% end %>
</div>