<div class="container-fluid main-content">
  <div class="banner">
<!--    <img src="banner.jpg" alt="banner">-->
    <%= image_tag "banner.jpg" %>
  </div>


<!--  LOGIN PAGE FOR VOTER ON HOME PAGE-->

  <div class="login-side">
    <div class="login-card">
      <h3>Voter Login</h3>
      <form>
        <div class="mb-3">
          <label for="exampleInputEmail1" class="form-label">Email address</label>
          <input type="email" name="voter_email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp">
          <div id="emailHelp" class="form-text">We'll never share your email with anyone else.</div>
        </div>
        <div class="mb-3">
          <label for="exampleInputPassword1" class="form-label">Password</label>
          <input type="password" name="voter_password" class="form-control" id="exampleInputPassword1">
        </div>
        <button type="submit" class="btn btn-primary " style="width: 100%">Login</button>
      </form>
    </div>
  </div>
</div>

